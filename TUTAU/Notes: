alter table tutau_gaia_all rename ra_icrs to ora;
alter table tutau_gaia_all rename de_icrs to odec ;

select ora::numeric(9,5) as "RA",odec::numeric(9,5) as "Dec",
       r2s(ora),d2s(odec),
       gmag::numeric(7,3),teff::numeric(7,3),
       (60*q3c_dist(ora,odec,86.307187, +24.42012))::numeric(7,3) as "dist[arcmin]"
from tutau_gaia_all
   where q3c_dist(ora,odec,86.307187, +24.42012) < 0.25
   and gmag < 15
order by teff;

q3c_dist(ora,odec,86.307187, +24.42012);


